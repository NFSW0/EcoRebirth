; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Eco Rebirth"
run/main_scene="res://launcher/scenes/launcher.tscn"
config/use_custom_user_dir=true
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://icon.svg"

[audio]

buses/default_bus_layout="res://audio/assets/AudioBus/default_bus_layout.tres"

[autoload]

DataManager="*res://data/scripts/data_manager.gd"
AudioManager="*res://audio/scripts/audio_manager.gd"
UIManager="*res://ui/scripts/ui_manager.gd"

[display]

window/size/viewport_width=480
window/size/viewport_height=270
window/size/window_width_override=960
window/size/window_height_override=540
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[gui]

theme/default_theme_scale=0.5
theme/custom="res://ui/assets/EcoRebirth.tres"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
